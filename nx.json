{
  "npmScope": "moxii",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ],
        "accessToken": "NmE2ODYxNjMtMjgxNC00ZTY3LWI1MTItMzdmNWJlYTNiNzkyfHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "data": {
      "tags": ["scope:shared"]
    },
    "moxii-api": {
      "tags": ["scope:api"],
      "implicitDependencies": ["data"]
    },
    "moxii-web": {
      "tags": ["scope:client"]
    },
    "moxii-web-e2e": {
      "tags": [],
      "implicitDependencies": [
        "moxii-web"
      ]
    }
  }
}

{
  "version": 2,
  "projects": {
    "moxii-api": {
      "root": "apps/moxii-api",
      "sourceRoot": "apps/moxii-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/moxii-api",
            "main": "apps/moxii-api/src/main.ts",
            "tsConfig": "apps/moxii-api/tsconfig.app.json",
            "assets": [
              "apps/moxii-api/src/assets"
            ],
            "generatePackageJson": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/moxii-api/src/environments/environment.ts",
                  "with": "apps/moxii-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "moxii-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/moxii-api/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/moxii-api"
          ],
          "options": {
            "jestConfig": "apps/moxii-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "moxii-web": {
      "root": "apps/moxii-web",
      "sourceRoot": "apps/moxii-web/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/moxii-web",
            "index": "apps/moxii-web/src/index.html",
            "main": "apps/moxii-web/src/main.tsx",
            "polyfills": "apps/moxii-web/src/polyfills.ts",
            "tsConfig": "apps/moxii-web/tsconfig.app.json",
            "assets": [
              "apps/moxii-web/src/favicon.ico",
              "apps/moxii-web/src/assets"
            ],
            "styles": [
              "apps/moxii-web/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/moxii-web/src/environments/environment.ts",
                  "with": "apps/moxii-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "moxii-web:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "moxii-web:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/moxii-web/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/moxii-web"
          ],
          "options": {
            "jestConfig": "apps/moxii-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "moxii-web-e2e": {
      "root": "apps/moxii-web-e2e",
      "sourceRoot": "apps/moxii-web-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/moxii-web-e2e/cypress.json",
            "tsConfig": "apps/moxii-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "moxii-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "moxii-web:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/moxii-web-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "shared": {
      "root": "libs/shared",
      "sourceRoot": "libs/shared/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/shared",
            "main": "libs/shared/src/index.ts",
            "tsConfig": "libs/shared/tsconfig.lib.json",
            "assets": [
              "libs/shared/*.md"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared"
          ],
          "options": {
            "jestConfig": "libs/shared/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "orders-api": {
      "projectType": "application",
      "root": "apps/orders-api",
      "sourceRoot": "apps/orders-api",
      "targets": {
        "build": {
          "executor": "@nx-go/nx-go:build",
          "options": {
            "outputPath": "dist/apps/orders-api",
            "main": "apps/orders-api/main.go"
          }
        },
        "serve": {
          "executor": "@nx-go/nx-go:serve",
          "options": {
            "main": "apps/orders-api/main.go"
          }
        },
        "test": {
          "executor": "@nx-go/nx-go:test"
        },
        "lint": {
          "executor": "@nx-go/nx-go:lint"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "moxii-api",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  }
}
